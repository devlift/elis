<?xml version="1.0" encoding="UTF-8" ?>
<XMLDB PATH="elis/program/plugins/results_engine" VERSION="20110926" COMMENT="XMLDB file for Moodle ELIS Program Manager Results Engine plug-in"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:noNamespaceSchemaLocation="../../../../lib/xmldb/xmldb.xsd"
>
  <TABLES>
    <TABLE NAME="crlm_results" COMMENT="results engine settings" NEXT="crlm_results_action">
      <FIELDS>
        <FIELD NAME="id" TYPE="int" LENGTH="10" NOTNULL="true" UNSIGNED="true" SEQUENCE="true" NEXT="contextid"/>
        <FIELD NAME="contextid" TYPE="int" LENGTH="10" NOTNULL="true" UNSIGNED="true" SEQUENCE="false" PREVIOUS="id" NEXT="active"/>
        <FIELD NAME="active" TYPE="int" LENGTH="1" NOTNULL="true" UNSIGNED="true" DEFAULT="0" SEQUENCE="false" PREVIOUS="contextid" NEXT="eventtriggertype"/>
        <FIELD NAME="eventtriggertype" TYPE="int" LENGTH="1" NOTNULL="true" UNSIGNED="true" DEFAULT="0" SEQUENCE="false" PREVIOUS="active" NEXT="lockedgrade"/>
        <FIELD NAME="lockedgrade" TYPE="int" LENGTH="1" NOTNULL="true" UNSIGNED="true" DEFAULT="0" SEQUENCE="false" PREVIOUS="eventtriggertype" NEXT="triggerstartdate"/>

        <FIELD NAME="triggerstartdate" TYPE="int" LENGTH="1" NOTNULL="true" UNSIGNED="true" DEFAULT="0" SEQUENCE="false" COMMENT="Indicates what date to base the trigger on" PREVIOUS="lockedgrade" NEXT="days"/>
        <FIELD NAME="days" TYPE="int" LENGTH="2" NOTNULL="true" UNSIGNED="true" DEFAULT="0" SEQUENCE="false" COMMENT="Number of days to offset from trigger date" PREVIOUS="triggerstartdate" NEXT="criteriatype"/>
        <FIELD NAME="criteriatype" TYPE="int" LENGTH="1" NOTNULL="true" UNSIGNED="true" DEFAULT="0" SEQUENCE="false" PREVIOUS="days"/>
      </FIELDS>
      <KEYS>
        <KEY NAME="primary" TYPE="primary" FIELDS="id"/>
      </KEYS>
      <INDEXES>
        <INDEX NAME="contextid_idx" UNIQUE="true" FIELDS="contextid"/>

      </INDEXES>
    </TABLE>
    <TABLE NAME="crlm_results_action" COMMENT="results engine action settings" PREVIOUS="crlm_results" NEXT="crlm_results_class_log">
      <FIELDS>
        <FIELD NAME="id" TYPE="int" LENGTH="10" NOTNULL="true" UNSIGNED="true" SEQUENCE="true" NEXT="resultengineid"/>
        <FIELD NAME="resultengineid" TYPE="int" LENGTH="10" NOTNULL="true" UNSIGNED="true" SEQUENCE="false" PREVIOUS="id" NEXT="actiontype"/>
        <FIELD NAME="actiontype" TYPE="int" LENGTH="1" NOTNULL="true" UNSIGNED="true" DEFAULT="0" SEQUENCE="false" PREVIOUS="resultengineid" NEXT="minimum"/>
        <FIELD NAME="minimum" TYPE="int" LENGTH="10" NOTNULL="true" UNSIGNED="false" SEQUENCE="false" PREVIOUS="actiontype" NEXT="maximum"/>
        <FIELD NAME="maximum" TYPE="int" LENGTH="10" NOTNULL="true" UNSIGNED="false" SEQUENCE="false" PREVIOUS="minimum" NEXT="trackid"/>

        <FIELD NAME="trackid" TYPE="int" LENGTH="10" NOTNULL="true" UNSIGNED="true" DEFAULT="0" SEQUENCE="false" PREVIOUS="maximum" NEXT="classid"/>
        <FIELD NAME="classid" TYPE="int" LENGTH="10" NOTNULL="true" UNSIGNED="true" DEFAULT="0" SEQUENCE="false" PREVIOUS="trackid" NEXT="fieldid"/>
        <FIELD NAME="fieldid" TYPE="int" LENGTH="10" NOTNULL="true" UNSIGNED="true" DEFAULT="0" SEQUENCE="false" PREVIOUS="classid" NEXT="fieldata"/>
        <FIELD NAME="fieldata" TYPE="char" LENGTH="255" NOTNULL="true" SEQUENCE="false" PREVIOUS="fieldid"/>
      </FIELDS>
      <KEYS>
        <KEY NAME="primary" TYPE="primary" FIELDS="id"/>
      </KEYS>
      <INDEXES>

        <INDEX NAME="resultengineid_idx" UNIQUE="false" FIELDS="resultengineid"/>
      </INDEXES>
    </TABLE>
    <TABLE NAME="crlm_results_class_log" COMMENT="results engine class logs" PREVIOUS="crlm_results_action" NEXT="crlm_results_student_log">
      <FIELDS>
        <FIELD NAME="id" TYPE="int" LENGTH="10" NOTNULL="true" UNSIGNED="true" SEQUENCE="true" NEXT="classid"/>
        <FIELD NAME="classid" TYPE="int" LENGTH="10" NOTNULL="true" UNSIGNED="true" SEQUENCE="false" PREVIOUS="id" NEXT="datescheduled"/>
        <FIELD NAME="datescheduled" TYPE="char" LENGTH="50" NOTNULL="true" SEQUENCE="false" PREVIOUS="classid" NEXT="daterun"/>
        <FIELD NAME="daterun" TYPE="char" LENGTH="50" NOTNULL="true" SEQUENCE="false" PREVIOUS="datescheduled"/>

      </FIELDS>
      <KEYS>
        <KEY NAME="primary" TYPE="primary" FIELDS="id"/>
      </KEYS>
      <INDEXES>
        <INDEX NAME="classid_idx" UNIQUE="false" FIELDS="classid"/>
      </INDEXES>
    </TABLE>
    <TABLE NAME="crlm_results_student_log" COMMENT="results engine student logs" PREVIOUS="crlm_results_class_log">

      <FIELDS>
        <FIELD NAME="id" TYPE="int" LENGTH="10" NOTNULL="true" UNSIGNED="true" SEQUENCE="true" NEXT="classlogid"/>
        <FIELD NAME="classlogid" TYPE="int" LENGTH="10" NOTNULL="true" UNSIGNED="true" SEQUENCE="false" PREVIOUS="id" NEXT="userid"/>
        <FIELD NAME="userid" TYPE="int" LENGTH="10" NOTNULL="true" UNSIGNED="true" SEQUENCE="false" PREVIOUS="classlogid" NEXT="action"/>
        <FIELD NAME="action" TYPE="char" LENGTH="255" NOTNULL="true" SEQUENCE="false" PREVIOUS="userid" NEXT="daterun"/>
        <FIELD NAME="daterun" TYPE="char" LENGTH="50" NOTNULL="true" SEQUENCE="false" PREVIOUS="action"/>
      </FIELDS>
      <KEYS>
        <KEY NAME="primary" TYPE="primary" FIELDS="id"/>

      </KEYS>
      <INDEXES>
        <INDEX NAME="classlogid_idx" UNIQUE="false" FIELDS="classlogid"/>
      </INDEXES>
    </TABLE>
  </TABLES>
</XMLDB>